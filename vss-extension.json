{
    "manifestVersion": 1,
    "id": "akvcodesign",
    "name": "Azure Key Vault Code Sign",
    "version": "0.0.1",
    "publisher": "PowerONPlatforms",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Azure Key Vault Code Signing DevOps Pipeline Task",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/PONLogoWhite256px.ico"        
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/PowerON-UK/AKV-Code-Sign"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "akvcodesign",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}